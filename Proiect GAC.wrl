DEF	BecuriCasa Transform {
	children [
		DEF CYCLE2 Script {
   eventIn SFTime touchTime
   field SFInt32 number 2
   eventOut SFInt32 output
   url "javascript:
      function initialize() {
         output = 0;
      }
      function touchTime(value, time) {
         if (output == number - 1) output = 0;
         else ++output;
      }
   "
}



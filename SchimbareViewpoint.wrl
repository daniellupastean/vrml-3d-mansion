#VRML V2.0 utf8

DEF White Appearance{
	material Material {
		diffuseColor 1.00 1.00 1.00
		transparency 0.4
	}
}


DEF Negru Appearance{
	material Material {
		diffuseColor 0 0 0
		transparency 0.7
	}
}

#Fata Poarta Mica

DEF View0 Viewpoint {
   position 1 1 12.8
   jump	FALSE
}

Transform{
	children[
		DEF touch0 TouchSensor {}
		Transform {
			translation	1 0.2 9.8
			children [
		 
				 Shape{
					appearance USE White
		    		geometry Cylinder{
					radius 0.5
		    		height 0.02
		    		}
				}
			]
		}

		Transform {
			translation	1 0.2 4.8
			children [
		 
				 Shape{
					appearance USE White
		    		geometry Cylinder{
					radius 0.2
		    		height 0.02
		    		}
				}
			]
		}

		Transform {
			translation	-3.8 0.2 9.8
			children [
		 
				 Shape{
					appearance USE White
		    		geometry Cylinder{
					radius 0.2
		    		height 0.02
		    		}
				}
			]
		}
	]
}



DEF touch0Script Script {
   eventIn SFBool touch0IsActive
   eventOut SFBool bindView0
   
   url [ "javascript:
		function touch0IsActive(active) {
			bindView0 = TRUE;
		}",
	 "vrmlscript:
		function touch0IsActive(active) {
			bindView0 = TRUE;
		}",
	 ]
}


ROUTE touch0.isActive TO touch0Script.touch0IsActive
ROUTE touch0Script.bindView0 TO View0.set_bind


